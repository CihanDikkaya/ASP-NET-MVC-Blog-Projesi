@using WebApplication1.Models.Sınıflar

@model List<Hakkimizda>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}



<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var deneme in Model)
            {
                <img src="@deneme.FotoUrl" alt="" />

                <p>@deneme.Aciklama</p>
            }

        </div>
    </div>
</div>
