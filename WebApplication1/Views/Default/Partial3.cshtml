@using WebApplication1.Models.Sınıflar
@model List<Blog>



<div class="padding">
    <h4>En Popüler 5 Blog</h4>
    <ul class="list list-pad">
        @{var i = 0; }
        @foreach (var item in Model)
        {
            <li class="list-bg"><span>@{i++;} @i.</span><a href="/Blog/BlogDetay/@item.ID">@item.Baslik</a></li>
        }


    </ul>
    <a href="/Blog/Index" class="link1">Tüm blogları Gör</a>
</div>
